{
  "name": "lokalise-actions",
  "version": "0.1.0",
  "private": true,
  "description": "Push to and pull from Lokalise",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/color/lokalise-actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "~1.10.0",
    "@lokalise/node-api": "~8.2.0",
    "got": "~11.8.2",
    "json-stable-stringify": "~1.0.2",
    "pofile": "~1.1.4 ",
    "unzipper": "~0.10.11"
  },
  "devDependencies": {
    "@types/jest": "~29.2.6",
    "@types/json-stable-stringify": "~1.0.34",
    "@types/node": "~16.18.11",
    "@types/unzipper": "~0.10.5",
    "@typescript-eslint/parser": "~5.57.0",
    "@typescript-eslint/eslint-plugin": "~5.48.2",
    "eslint-import-resolver-typescript":"~3.5.3",
    "@vercel/ncc": "~0.36.0",
    "eslint": "~8.32.0",
    "eslint-plugin-github": "~4.6.0",
    "eslint-plugin-jest": "~27.2.1",
    "jest": "~29.3.1",
    "jest-circus": "~29.3.1",
    "js-yaml": "~4.1.0",
    "prettier": "~2.8.3",
    "ts-jest": "~29.0.5",
    "typescript": "~4.9.4"
  }
}
